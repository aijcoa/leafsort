{
  "name": "leafview",
  "productName": "LeafView",
  "description": "A minimalist image viewer based on Leaflet.js and Electron.",
  "version": "1.7.7",
  "license": "MIT",
  "author": {
    "name": "sprout2000",
    "url": "https://github.com/sprout2000",
    "email": "sogacey512@iistoria.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/leafview.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "predev": "rimraf dist",
    "dev": "run-p dev:*",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"development\" electron .",
    "serve": "electron .",
    "clean": "rimraf dist release",
    "start": "run-s clean build serve",
    "build": "cross-env NODE_ENV=\"production\" webpack",
    "build:package": "ts-node scripts/build.ts",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --write .",
    "release": "ts-node scripts/build.ts",
    "package": "run-s clean build build:package",
    "prepare": "git config --local core.hooksPath hooks"
  },
  "dependencies": {
    "electron-log": "^4.4.6",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5",
    "i18next": "^21.6.14",
    "leaflet": "^1.7.1",
    "mime-types": "^2.1.35",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.9",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.0",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "electron-notarize": "^1.1.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-search-devtools": "^1.6.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "4.6.2",
    "wait-on": "^6.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-regexp": "^1.5.1",
    "prettier": "^2.5.1"
  }
}
