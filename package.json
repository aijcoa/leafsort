{
  "name": "leafview",
  "productName": "LeafView",
  "version": "0.3.1",
  "main": "dist/main.js",
  "description": "Secure image viewer for minimalists based on leaflet.js",
  "author": {
    "name": "sprout2000",
    "url": "https://github.com/sprout2000"
  },
  "homepage": "https://sprout2000.github.io/leafview/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/leafview.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/leafview/issues"
  },
  "keywords": [
    "image-viewer",
    "electron",
    "pan",
    "zoom",
    "cross-platform"
  ],
  "engines": {
    "node": "12.x",
    "yarn": ">=1.22.0",
    "npm": "please_use_yarn_instead"
  },
  "os": [
    "win32",
    "darwin"
  ],
  "scripts": {
    "start": "run-s clean build serve",
    "serve": "electron .",
    "clean": "rimraf dist release",
    "build": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack --progress",
    "release": "node scripts/build.js",
    "prepackage": "run-s clean build:prod",
    "prepackage:appx": "run-s clean build:prod",
    "package": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false node scripts/build-unsign.js",
    "package:appx": "node scripts/build.js",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:push": "git push --follow-tags origin master",
    "deploy": "gh-pages -d public"
  },
  "dependencies": {
    "electron-log": "^4.2.2",
    "electron-updater": "^4.3.1",
    "electron-window-state": "^5.0.3",
    "howler": "^2.2.0",
    "i18next": "^19.5.6",
    "leaflet": "^1.6.0",
    "mime-types": "^2.1.27",
    "natsort": "^2.0.2",
    "react": "^16.13.1",
    "react-device-detect": "^1.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-resize-detector": "^5.0.6"
  },
  "devDependencies": {
    "@types/electron-load-devtool": "^1.2.0",
    "@types/howler": "^2.2.1",
    "@types/leaflet": "^1.5.16",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-resize-detector": "^4.2.0",
    "@types/styled-components": "^5.1.1",
    "@types/webpack": "^4.41.21",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "electron": "^8.4.0",
    "electron-builder": "^22.7.0",
    "electron-load-devtool": "^1.2.0",
    "electron-notarize": "^1.0.0",
    "html-webpack-plugin": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "styled-components": "^5.1.1",
    "terser-webpack-plugin": "^3.0.6",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gh-pages": "^3.1.0",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.1"
  }
}
